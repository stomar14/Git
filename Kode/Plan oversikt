*/
	function showimage()
	{
		if (!document.images)
		return
		document.images.pictures.src=
		document.mygallery.picture.options[document.mygallery.picture.selectedIndex].value
	}
</script>
</head>

<body>
	<table border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td width="100%"><form name="mygallery"><p>
			<select name="picture" size="1" onChange="showimage()">
				<option selected value="Prosjekt/images/etasjeEn.jpg">1. Etasje</option>
				<option value="Prosjekt/images/etasjeTo.jpg">2. Etasje</option>
			</select>
		</p>
	</form>
	</td>
	</tr>
<tr>
	<td width="100%"><p align="center"><img src="Prosjekt/images/etasjeEn.jpg" name="pictures"
	 width="500" height="300"></td>
</tr>
</table>
